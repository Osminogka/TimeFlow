lPages = ref(1);\r\nconst pageSize = 10; // Users per page\r\n\r\n// Fetch users (with optional search)\r\nconst fetchUsers = async () => {\r\n  console.log('Fetching users...');\r\n};\r\n\r\n// Watch for search input changes\r\nwatch(searchQuery, () => {\r\n  currentPage.value = 1; // Reset to first page on search\r\n  fetchUsers();\r\n});\r\n\r\n// Handle pagination\r\nconst nextPage = () => {\r\n  if (currentPage.value < totalPages.value) {\r\n    currentPage.value++;\r\n    fetchUsers();\r\n  }\r\n};\r\n\r\nconst prevPage = () => {\r\n  if (currentPage.value > 1) {\r\n    currentPage.value--;\r\n    fetchUsers();\r\n  }\r\n};\r\n\r\n// Fetch users on mount\r\nonMounted(fetchUsers);\r\n</script>\r\n\r\n<template>\r\n  <div class=\"search-container\">\r\n    <input v-model=\"searchQuery\" placeholder=\"Search users...\" class=\"search-input\" />\r\n\r\n    <div v-if=\"users.length > 0\" class=\"users-list\">\r\n      <div v-for=\"user in users\" :key=\"user.id\" class=\"user-card\">\r\n        <div class=\"avatar\">{{ user.username.charAt(0).toUpperCase() }}</div>\r\n        <div class=\"user-info\">\r\n          <h3>{{ user.username }}</h3>\r\n          <p>{{ user.email }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else class=\"no-users\">No users found</div>\r\n\r\n    <!-- Pagination Controls -->\r\n    <div class=\"pagination\" v-if=\"totalPages > 1\">\r\n      <button @click=\"prevPage\" :disabled=\"currentPage === 1\" class=\"page-btn\">← Prev</button>\r\n      <span class=\"page-indicator\">Page {{ currentPage }} of {{ totalPages }}</span>\r\n      <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\" class=\"page-btn\">Next →</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.search-container {\r\n  width: 100%;\r\n  max-width: 600px;\r\n  margin: auto;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.search-input {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border: 2px solid #36A2