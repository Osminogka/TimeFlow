;\r\n    users.value = response.data.users;\r\n    totalPages.value = response.data.totalPages;\r\n  } catch (error) {\r\n    console.error('Error fetching users:', error);\r\n  }\r\n};\r\n\r\n// Watch for search input changes\r\nwatch(searchQuery, () => {\r\n  currentPage.value = 1; // Reset to first page on search\r\n  fetchUsers();\r\n});\r\n\r\n// Handle pagination\r\nconst nextPage = () => {\r\n  if (currentPage.value < totalPages.value) {\r\n    currentPage.value++;\r\n    fetchUsers();\r\n  }\r\n};\r\n\r\nconst prevPage = () => {\r\n  if (currentPage.value > 1) {\r\n    currentPage.value--;\r\n    fetchUsers();\r\n  }\r\n};\r\n\r\n// Fetch users on mount\r\nonMounted(fetchUsers);\r\n</script>\r\n\r\n<template>\r\n  <div class=\"container\">\r\n    <header class=\"header\">\r\n      <h1 class=\"header-text\">User Search</h1>\r\n    </header>\r\n\r\n    <main class=\"content\">\r\n      <input v-model=\"searchQuery\" placeholder=\"Search users...\" class=\"search-input\" />\r\n\r\n      <div v-if=\"users.length > 0\" class=\"users-list\">\r\n        <div v-for=\"user in users\" :key=\"user.username\" class=\"user-card\">\r\n          <div class=\"avatar\">{{ user.username.charAt(0).toUpperCase() }}</div>\r\n          <div class=\"user-info\">\r\n            <h3>{{ user.username }}</h3>\r\n            <p>{{ user.email }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-else class=\"no-users\">No users found</div>\r\n\r\n      <!-- Pagination Controls -->\r\n      <div class=\"pagination\" v-if=\"totalPages > 1\">\r\n        <button @click=\"prevPage\" :disabled=\"currentPage === 1\" class=\"page-btn\">← Prev</button>\r\n        <span class=\"page-indicator\">Page {{ currentPage }} of {{ totalPages }}</span>\r\n        <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\" class=\"page-btn\">Next →</button>\r\n      </div>\r\n    </main>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n@import '@/assets/css/animations.css'; /* Ensure animations are used */\r\n\r\n.container {\r\n  width: 100%;\r\n  max-width: 600px;\r\n  margin: auto;\r\n  padding: 20px;\r\n  text-align: center;\r\n  background: #1e1e2e;\r\n  border-radius: 15px;\r\n  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);\r\n  color: white;\r\n}\r\n\r\n/* Header */\r\n.header {\r\n  padding: 1rem;\r\n  background: linear-gradient(90deg, #ff007f, #ff5e62);\r\n  border-radius: 10px 10px 0 0;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.header-text {\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n  background: linear-gradient(90deg, #ff007f, #ff5e62);\r\n}\r\n\r\n/* Search Input */\r\n.search-input {\r\n  width: 100%;\r\n  padding: 12px;\r\n  border: none;\r\n  border-radius: 8px;\r\n  font-size: 16px;\r\n  outline: none;\r\n  background: #252638;\r\n  color: white;\r\n  box-shadow: inset 0 2px 5px rgba(255, 255, 255, 0.1);\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.search-input:focus {\r\n  box-shadow: 0 0 10px rgba(255, 0, 127, 0.5);\r\n}\r\n\r\n/* Users List */\r\n.users-list {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 10px;\r\n}\r\n\r\n.user-card {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 15px;\r\n  border-radius: 10px;\r\n  background: #252638;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.3);\r\n  transition: transform 0.2s ease, box-shadow 0.2s ease;\r\n}\r\n\r\n.user-card:hover {\r\n  transform: scale(1.03);\r\n  box-shadow: 0 6px 15px rgba(255, 0, 127, 0.3);\r\n}\r\n\r\n/* Avatar */\r\n.avatar {\r\n  width: 50px;\r\n  height: 50px;\r\n  border-radius: 50%;\r\n  background: linear-gradient(90deg, #ff007f, #ff5e62);\r\n  color: white;\r\n  font-weight: bold;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-size: 1.2rem;\r\n  margin-right: 15px;\r\n}\r\n\r\n/* User Info */\r\n.user-info h3 {\r\n  margin: 0;\r\n  font-size: 18px;\r\n  color: white;\r\n}\r\n\r\n.user-info p {\r\n  margin: 5px 0 0;\r\n  font-size: 14px;\r\n  color: #b3b3b3;\r\n}\r\n\r\n/* No Users */\r\n.no-users {\r\n  margin-top: 20px;\r\n  color: #ff5e62;\r\n}\r\n\r\n/* Pagination */\r\n.pagination {\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 15px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.page-btn {\r\n  background: linear-gradient(90deg, #ff007f, #ff5e62);\r\n  color: white;\r\n  border: none;\r\n  padding: 10px 16px;\r\n  border-radius: 8px;\r\n  font-weight: bold;\r\n  font-size: 14px;\r\n  cursor: pointer;\r\n  transition: transform 0.2s ease, opacity 0.2s ease;\r\n  box-shadow: 0px 4px 10px rgba(255, 0, 127, 0.3);\r\n}\r\n\r\n.page-btn:disabled {\r\n  background: gray;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.page-btn:hover {\r\n  transform: scale(1.05);\r\n  opacity: 0.9;\r\n}\r\n\r\n.page-indicator {\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  color: white;\r\n}\r\n</style>\r\n"],"mappings":";;EAmDOA,KAAK,EAAC;AAAW;;EAKdA,KAAK,EAAC;AAAS;;EAxDzBC,GAAA;EA2DmCD,KAAK,EAAC;;;EAE1BA,KAAK,EAAC;AAAQ;;EACdA,KAAK,EAAC;AAAW;;EA9DhCC,GAAA;EAqEkBD,KAAK,EAAC;;;EArExBC,GAAA;EAwEWD,KAAK,EAAC;;mBAxEjB;;EA0EcA,KAAK,EAAC;AAAgB;oBA1EpC;;uBAmDEE,mBAAA,CA2BM,OA3BNC,UA2BM,G,0BA1BJC,mBAAA,CAES;IAFDJ,KAAK,EAAC;EAAQ,IACpBI,mBAAA,CAAwC;IAApCJ,KAAK,EAAC;EAAa,GAAC,aAAW,E,sBAGrCI,mBAAA,CAqBO,QArBPC,UAqBO,G,gBApBLD,mBAAA,CAAkF;IAzDxF,uBAAAE,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAyDsBC,MAAA,CAAAC,WAAW,GAAAF,MAAA;IAAEG,WAAW,EAAC,iBAAiB;IAACV,KAAK,EAAC;iDAAjDQ,MAAA,CAAAC,WAAW,E,GAEhBD,MAAA,CAAAG,KAAK,CAACC,MAAM,Q,cAAvBV,mBAAA,CAQM,OARNW,UAQM,I,kBAPJX,mBAAA,CAMMY,SAAA,QAlEdC,WAAA,CA4D4BP,MAAA,CAAAG,KAAK,EAAbK,IAAI;yBAAhBd,mBAAA,CAMM;MANsBD,GAAG,EAAEe,IAAI,CAACC,QAAQ;MAAEjB,KAAK,EAAC;QACpDI,mBAAA,CAAqE,OAArEc,UAAqE,EAAAC,gBAAA,CAA9CH,IAAI,CAACC,QAAQ,CAACG,MAAM,IAAIC,WAAW,oBAC1DjB,mBAAA,CAGM,OAHNkB,UAGM,GAFJlB,mBAAA,CAA4B,YAAAe,gBAAA,CAArBH,IAAI,CAACC,QAAQ,kBACpBb,mBAAA,CAAuB,WAAAe,gBAAA,CAAjBH,IAAI,CAACO,KAAK,iB;qDAKtBrB,mBAAA,CAAiD,OAAjDsB,UAAi